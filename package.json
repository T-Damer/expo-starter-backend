{
  "name": "graphql-backend-starter",
  "version": "1.0.0",
  "description": "Graphql backend server starter template",
  "main": "dist/server.js",
  "repository": "https://github.com/T-Damer/expo-backend-starter",
  "author": "t-damer <dpancov64@gmail.com>",
  "prettier": "@big-whale-labs/prettier-config",
  "license": "MIT",
  "scripts": {
    "distribute": "yarn build && node dist/server.js",
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
    "build": "tsc --skipLibCheck",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "lint-fix": "yarn eslint ./src/ --fix",
    "generate-schema": "prisma format && prisma generate"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "5.10.2",
    "axios": "^1.6.7",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.5",
    "graphql-yoga": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "prisma": "^5.10.2",
    "reflect-metadata": "^0.2.1",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.27.2"
  },
  "devDependencies": {
    "@big-whale-labs/bwl-eslint-backend": "^1.0.5",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/pg": "^8",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.57.0",
    "module-alias": "^2.2.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@4.1.0"
}
